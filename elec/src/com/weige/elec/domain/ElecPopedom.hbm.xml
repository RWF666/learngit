<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.weige.elec.domain.ElecPopedom" table="Elec_Popedom">
		<composite-id>
           <key-property name="mid" type="string">
               <column name="mid"/>
           </key-property>
           <key-property name="pid" type="string">
               <column name="pid"/>
           </key-property>
        </composite-id>		
		<property name="name" type="string" column="name"></property>
		<property name="url" type="string" column="url"></property>
		<property name="icon" type="string" column="icon"></property>
		<property name="target" type="string" column="target"></property>
		<property name="isParent" type="boolean" column="isParent"></property>
		<property name="isMenu" type="boolean" column="isMenu"></property>
	</class>
</hibernate-mapping>
